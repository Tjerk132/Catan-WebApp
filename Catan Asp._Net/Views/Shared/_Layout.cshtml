<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Map Generator</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="https://fonts.googleapis.com/css?family=Karla" rel="stylesheet">
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="https://fonts.googleapis.com/css?family=Lusitana:700" rel="stylesheet">
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
</head>
<body>
    <ul class="unselectable">
        <div class="title">
            <p style="font-size:55px; font-family: 'Lusitana', serif;">CATAN</p><br>
            <hr>
            <p style="font-size: 28px; font-family: 'Karla', sans-serif;">MAP GENERATOR</p>
            <button class="ctn-btn" onClick="window.location.reload()">Generate</button>
        </div>
        <div class="generate-controller">
        </div>
    </ul>
    <div class="container body-content">
        @RenderBody()
    </div>

    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
</body>
</html>